begin struct Node: 
    ref Node next;
    int value;
end Node;

begin proc(int a, int b):
    begin if (a>b):
        print(a);
    end if;
    begin if (a<b):
        print(b);
    end if;
    begin if (a==b):
        print(a+b);
    end if;
end proc;

begin int main():
    ref Node n1 = create Node;
    n1.value = 2;
    proc(n1.value, 5);
    return 1;
end main;
